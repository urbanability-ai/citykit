{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.invalid/openclaw-city-demo-kit/schemas/dataset_manifest.schema.json",
  "title": "DatasetManifest",
  "type": "object",
  "required": ["schema_version", "dataset_id", "created_at_utc", "provenance", "outputs"],
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "dataset_id": {
      "type": "string"
    },
    "created_at_utc": {
      "type": "string"
    },
    "provenance": {
      "type": "object",
      "required": ["map", "attribution"],
      "properties": {
        "map": {
          "type": "string"
        },
        "attribution": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "outputs": {
      "type": "object",
      "required": ["scenario", "map", "multiview", "labels", "pcd_groundtruth", "pcd_pseudo"],
      "properties": {
        "scenario": {
          "type": "string"
        },
        "map": {
          "type": "string"
        },
        "multiview": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string"
          }
        },
        "labels": {
          "type": "string"
        },
        "pcd_groundtruth": {
          "type": "string"
        },
        "pcd_pseudo": {
          "type": "string"
        }
      },
      "additionalProperties": true
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": true
}
